pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

--[[
	Text fader
	Intended for introduction/startup screens

	Add a screen of text using ftxt_screen() and passing a string
	text as a argument.

	Create line breaks using the semicolon ; character.
	You must have a semicolon as the last character in the passed string.
	
	Example:
	ftxt_screen("line 1;line 2;line3;")

]]

--Add screen of text, call before ftxt_init()
function ftxt_screen(txt) add(ftxt_all, txt) end

ftxt_all={}
function ftxt_init(oncomplete)
	ftxt_st=1
	ftxt_colors={0,1,5,13,6,7}
	ftxt_func=oncomplete
	ftxt_reset()
	
	for k,txt in pairs(ftxt_all) do ftxt_all[k]=split(txt) end
end

function ftxt_reset()
	ftxt_t=0
	ftxt_pos=1
	ftxt_mode=1 --1=fadein,2=read,3=fadeout,4=waitout
end

function ftxt_draw()
	function mode(m) ftxt_mode=m ftxt_t=0 end
	
	ftxt_y=10
	ftxt_color=ftxt_colors[ftxt_pos]

	for line in all(ftxt_all[ftxt_st]) do
		
		if #line>0 then
			print(line, 64-(#line*2),ftxt_y, ftxt_color) --centers text
		end
		
		ftxt_y+=7
	end
	
	if ftxt_mode==1 or ftxt_mode==2 then --fadein/fadeout
		if ftxt_t>3 then
			ftxt_t=0
			
			if ftxt_mode==1 then
				ftxt_pos+=1
				if ftxt_pos>#ftxt_colors-1 then mode(3) end
			else
				ftxt_pos-=1
				if ftxt_pos<2 then mode(4) end
			end
		end
	end

	if ftxt_mode==3 then --read text
		if ftxt_t>60 then mode(2) end
	end

	if ftxt_mode==4 then --all done, next set or exit func
		if ftxt_t>30 then
			ftxt_st+=1
			if ftxt_st>#ftxt_all then
				ftxt_func()
			else
				ftxt_reset()
			end
		end
	end

	ftxt_t+=1
end

-- support functions (remove if this exists elsewhere in your cart)
-- split(string, delimter)
function split(s,dc)
	dc=dc or ";"
	local a={}
	local ns=""
	
	
	while #s>0 do
		local d=sub(s,1,1)
		if d==dc then
			add(a,ns)
			ns=""
		else
			ns=ns..d
		end
	
		s=sub(s,2)
	end
	
	return a
end



-- Below for demo only
-- This only requires init() and draw() - there is no function within update()

function _init()
	
	ftxt_screen("this is screen number 1;")
	ftxt_screen("this is screen number 2;;with an extra line;")
	ftxt_screen("use it for good;not evil;")
	ftxt_init(finish) --Passing function
end

function _draw()
	cls()
	ftxt_draw()
end

--This function is called when all the text screens are done displaying
function finish()
	print("text scenes finished",1,1,8)	
end



