--Explosion particle lib
--Brian Vaughn
--History: 11/14/16, 2/16/17

--[[
	Basic explosion particle library

	In-game use:
	expl_create(x,y, numberOfParticles, optionsTable, drawLayer)
    
    x                   X coordinate of origin
    y                   Y coordinate of origin
    numberOfParticles   Total number of particles to make
    optionsTable        Optional. Custom settings, see below.
    drawLayer           Optional. Draw layer you want your explosion to be on. Default: 1
    
	
    You only need to pass the options that you want to change
        optionsTable={
            dur=30,    -- Duration of particle in frames
            rad=0,     -- Starting radius of particle circle
            decay=.25, -- Rate of reduction per cycle
            colors={7,10,9}, -- Table of colors
            smin=.25, -- Minimum speed of particles
            smax=1,   -- Maximum speed of particles
            grav=.3,  -- Maximum gravity applied
            dir=0,    -- Direction of force. 0 for all directions
            range=0   -- Distribution of particles around the direction (half of value)
        }
	
	
	Add these functions to your system loops:
	expl_update()      Updates all explosions and layers
	expl_draw(layer)   Updates single draw layer. Default: 1
    
    You can call expl_draw() in multiple places assuming you provide a layer. This
    allows you to have explosions where you want them compared to other graphics. Just
    make sure you provide the layer number when you create your explosion.
]]

expl_all={}
function expl_create(x,y, qty, options, layer)
    layer=layer or 1

	for n=0,qty do
		local obj={
			x=x,y=y,
			t=0,
			dur=30,
			rad=0,
			decay=.25,
			dia=0,
			colors={7,10,9,8},
			smin=.25,
			smax=1,
			grav=.3,
			dir=0,
			range=0,
			layer=layer
		}
		
		if options then
			for k,v in pairs(options) do obj[k] = v end
		end
		
		local c=flr(rnd(#obj.colors))+1
		local sp=rnd(obj.smax-obj.smin)+obj.smin

		if obj.dir>0 then
			local dirh=obj.range/2
			local dira=obj.dir-dirh
			local dirb=obj.dir+dirh
			
			obj.dir=rnd(dirb-dira)+dira
		else
			obj.dir=rnd()	
		end
	
		obj.c=obj.colors[c]
		obj.g=rnd(abs(obj.grav))
		obj.dx=cos(obj.dir)*sp
		obj.dy=sin(obj.dir)*sp
		
		if obj.grav<0 then obj.g*=-1 end

		add(expl_all,obj)
	end
end

function expl_update()
	if #expl_all>0 then
		for o in all(expl_all) do
			o.dy+=o.g
			o.y+=o.dy
			o.x+=o.dx
			o.t+=1
			o.rad-=o.decay
			o.r=max(o.rad,0)

			if o.t>o.dur then del(expl_all,o) end
		end
	end
end

function expl_draw(layer)
    layer=layer or 1
	
	for e in all(expl_all) do
		if e.layer==layer then
			circfill(e.x,e.y, e.r, e.c)
		end
	end
	
end








-- CART DEMO

function _update()
	custom_cfg={dir=-1,range=.88}
	
	if btnp(0) then custom_cfg={dir=.5,range=.5,x=54,y=64,rad=3,smax=8} end
	if btnp(1) then custom_cfg={dir=1,range=.5,x=74,y=64,rad=3,smax=8} end
	if btnp(2) then custom_cfg={dir=.25,range=.5,x=64,y=54,rad=3,smax=8} end
	if btnp(3) then custom_cfg={dir=.75,range=.5,x=64,y=74,rad=3,smax=8} end
	if btnp(4) then custom_cfg={dir=0,rad=3,smax=8,grav=0,range=0} end

	if custom_cfg.dir>0 then
		expl_create(custom_cfg.x,custom_cfg.y, 24, custom_cfg, 1)
		
	end
	
	if custom_cfg.dir==0 then
		expl_create(64,64, 32, custom_cfg, 2)
	end

	expl_update()
	
end

debug=0

function _draw()
	cls()
	print("arrow keys explode in direction",1,5,5)
	print("z or \142 explodes in middle",1,13,5)
	
	
	expl_draw(1) --draws on layer 1, behind object
	rectfill(54,54, 74,74, 1)
	expl_draw(2) --draws on layer 2, in front of object
end






