pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

--Creates scanline glitch effect
function glitchfx(lines)
	local lines=lines or 64
	
	for i=1,lines do
		row=flr(rnd(128))
		row2=flr(rnd(127))
		if (row2>=row) row2+=1

		memcpy(0x4300, 0x6000+64*row, 64)
		memcpy(0x6000+64*row, 0x6000+64*row2, 64)
		memcpy(0x6000+64*row2, 0x4300,64)
	end
end



	
--
-- Demo
--
function _init()
	boxes={}
	for i=1,36 do
		add(boxes,{x=rnd(127),y=rnd(127),c=rnd(15)})
	end

end

	
function _draw()
	cls()
	
	for o in all(boxes) do
		rectfill(o.x,o.y,o.x+15,o.y+15,o.c)
	end
		
	print("z to glitch\nx to refresh",1,1,10)
		
	if btnp(4) then
		glitchfx() --number of lines to glitch
	end
		
	if btnp(5) then _init() end
end







